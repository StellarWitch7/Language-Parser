namespace test;

with core;
with core::threading;

func main() #i32 {
    local i #i32 = 1;

    while i != 100 {
        Write(Replace("$", "$", "t"));

        if i % 3 == 0 {
            Write("Fizz");
        }
        
        if i % 5 == 0 {
            Write("Buzz");
        }

        Write('\n');
        ++i;
    }

    return 0;
}